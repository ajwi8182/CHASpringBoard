<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.SpringBoard.Cha.SaramMapper">
	<insert id="insertSaram"><!-- 회원가입(회원등록) -->
		insert into saram values(#{id},#{passwd},#{name},#{email},#{joinDate},#{lastLogin},#{status})
	</insert>
	<select id="selectSaramList" resultType="Saram">
		select * from saram order by joinDate
	</select>
</mapper>